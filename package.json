{
  "name": "fill-all",
  "version": "1.0.0",
  "description": "Chrome extension to auto-fill forms using AI, rules, and Brazilian document generators",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "train:model": "tsx --tsconfig tsconfig.json scripts/train-model.ts",
    "import:rules": "tsx --tsconfig tsconfig.json scripts/import-rules-to-dataset.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@faker-js/faker": "^10.3.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.3.0",
    "@types/chrome": "^0.0.287",
    "@types/node": "^22.19.11",
    "@vitest/coverage-v8": "^4.0.18",
    "happy-dom": "^20.7.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "author": "marcusp",
  "license": "MIT"
}
